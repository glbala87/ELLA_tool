{
    "{DYNAMIC}/variants": {
        "presets": ["Test 1"],
        "limit_to_groups": null,
        "url": "/api/v1/igv/tracks/dynamic/variants/{ANALYSIS_ID}/?allele_ids={ALLELE_IDS}",
        "show": true,
        "igv": {
            "name": "Variants",
            "format": "vcf",
            "indexed": false,
            "order": 12,
            "visibilityWindow": 9999999999999
        }
    },
    "{DYNAMIC}/classifications": {
        "presets": [],
        "limit_to_groups": null,
        "url": "/api/v1/igv/tracks/dynamic/classifications/",
        "show": false,
        "igv": {
            "name": "Classifications",
            "format": "gff3",
            "indexed": false,
            "order": 11,
            "visibilityWindow": 9999999999999,
            "colorBy": "name",
            "colorTable": {
                "Class 1": "#76B100",
                "Class 2": "#6BA100",
                "Class 3": "#FFAA3C",
                "Class 4": "#FE5B5B",
                "Class 5": "#D00000",
                "*": "#888888"
            }
        }
    },
    "{DYNAMIC}/genepanel": {
        "presets": ["Test 1", "Test 2", "Test 3"],
        "limit_to_groups": null,
        "url": "/api/v1/igv/tracks/dynamic/genepanel/{GENEPANEL_NAME}/{GENEPANEL_VERSION}/",
        "show": true,
        "igv": {
            "name": "Gene panel",
            "type": "annotation",
            "format": "bed",
            "indexed": false,
            "displayMode": "EXPANDED",
            "order": 10,
            "height": 60
        }
    },
    "{ANALYSIS}/*.bam": {
        "limit_to_groups": null,
        "url": "/api/v1/igv/tracks/analyses/{ANALYSIS_ID}/{TRACK_FILEPATH}",
        "igv": {
            "format": "bam",
            "colorBy": "strand",
            "negStrandColor": "rgb(150,150,230)",
            "posStrandColor": "rgb(230,150,150)",
            "alignmentRowHeight": 12,
            "visibilityWindow": 20000,
            "order": 300
        }
    },
    "{ANALYSIS}/HG002.bam": {
        "igv": {
            "name": "BAM: Proband",
            "displayMode": "EXPANDED",
            "order": 1000
        }
    },
    "{ANALYSIS}/HG003.bam": {
        "igv": {
            "name": "BAM: Father",
            "displayMode": "EXPANDED",
            "order": 1001
        }
    },
    "{ANALYSIS}/HG004.bam": {
        "igv": {
            "name": "BAM: Mother",
            "displayMode": "EXPANDED",
            "order": 1002
        }
    },
    "{STATIC}/*": {
        "url": "/api/v1/igv/tracks/static/{TRACK_FILEPATH}"
    },
    "{STATIC}/*.bed.gz": {
        "igv": { "format": "bed", "displayMode": "EXPANDED", "order": 100 }
    },
    "{STATIC}/*.vcf.gz": {
        "igv": { "format": "vcf", "visibilityWindow": 1e9, "order": 200 }
    },
    "{STATIC}/*.bigWig": {
        "igv": { "format": "bigWig", "displayMode": "EXPANDED", "order": 200 }
    },
    "{STATIC}/*.bw": {
        "igv": { "format": "bigWig", "displayMode": "EXPANDED", "order": 200 }
    },
    "{STATIC}/*.gff3.gz": {
        "igv": { "format": "gff3", "displayMode": "EXPANDED", "order": 200 }
    },
    "{STATIC}/*.bam": {
        "igv": {
            "format": "bam",
            "colorBy": "strand",
            "negStrandColor": "rgb(150,150,230)",
            "posStrandColor": "rgb(230,150,150)",
            "alignmentRowHeight": 12,
            "visibilityWindow": 20000,
            "order": 300
        }
    },
    "{STATIC}/*.cram": {
        "igv": {
            "format": "cram",
            "colorBy": "strand",
            "negStrandColor": "rgb(150,150,230)",
            "posStrandColor": "rgb(230,150,150)",
            "alignmentRowHeight": 12,
            "visibilityWindow": 20000,
            "order": 300
        }
    },
    "{STATIC}/refGene*.bed.gz": {
        "presets": ["Test 3"],
        "show": false,
        "igv": {
            "name": "Refgene",
            "order": 1,
            "height": 100,
            "indexed": false
        }
    },
    "{STATIC}/uniprot.domains.bed.gz": {
        "limit_to_groups": ["testgroup01", "testgroup02"],
        "show": true,
        "igv": {
            "name": "Uniprot Domains",
            "order": 2
        }
    }
}
